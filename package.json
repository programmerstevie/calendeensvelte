{
  "name": "calendeensvelte",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "cf:prev": "npm run build && wrangler pages dev .svelte-kit/cloudflare",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "pulldb:dev": "npx drizzle-kit pull --config=drizzle-dev.config.ts",
    "pushdb:dev": "npx drizzle-kit push --config=drizzle-dev.config.ts",
    "pulldb:prod": "npx drizzle-kit pull --config=drizzle-prod.config.ts",
    "pushdb:prod": "npx drizzle-kit push --config=drizzle-prod.config.ts",
    "gen:email": "node script/generate-email-template.cjs"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241011.0",
    "@sveltejs/adapter-cloudflare": "^4.7.3",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/eslint": "^9.6.0",
    "@types/mjml": "^4.7.4",
    "@types/mustache": "^4.2.5",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.13",
    "drizzle-kit": "^0.25.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "postcss": "^8.4.47",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.19.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.3",
    "wrangler": "^3.80.4"
  },
  "type": "module",
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.0",
    "@auth/sveltekit": "^1.7.0",
    "@libsql/client": "^0.14.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.34.1",
    "svelte-icons": "^2.1.0",
    "tailwind-merge": "^2.5.3",
    "tw-to-css": "0.0.12"
  }
}
